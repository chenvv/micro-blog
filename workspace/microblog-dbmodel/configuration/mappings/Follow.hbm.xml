<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="vivian.jsash.microblog.dbmodel.dao">
	<class
		name="Follow"
		table="follow"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="INDEX_ID"
		>
			<generator class="identity"/>
		</id>

		<property
			name="AccountId"
			column="ACCOUNT_ID"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="FollowedId"
			column="FOLLOWED_ID"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>